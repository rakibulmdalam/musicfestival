	</servlet>
	<servlet>
		<servlet-name>NonAdminOverviewServlet</servlet-name>
		<servlet-class>de.tum.in.dbpra.controller.employee.NonAdminOverviewServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ShiftsServlet</servlet-name>
		<servlet-class>de.tum.in.dbpra.controller.employee.ShiftsServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>NonAdminNotesServlet</servlet-name>
		<servlet-class>de.tum.in.dbpra.controller.employee.NonAdminNotesServlet</servlet-class>
	</servlet>
