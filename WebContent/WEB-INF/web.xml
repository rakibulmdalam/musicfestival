		<url-pattern>/admin</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>BandSchedulerServlet</servlet-name>
		<url-pattern>/admin/schedules</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
